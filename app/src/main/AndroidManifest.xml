<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.guardianofgods.proprofile">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/ic_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProProfile"
        tools:targetApi="31">
        <activity
            android:name="com.guardianofgods.proprofile.ForgetPassword"
            android:exported="false" />
        <activity
            android:name="com.guardianofgods.proprofile.RegisterActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.guardianofgods.proprofile.LoginActivity"
            android:exported="true">

        </activity>
        <activity
            android:name="com.guardianofgods.proprofile.HomeActivity"
            android:exported="false" />
        <activity
            android:name="com.guardianofgods.proprofile.MainActivity"
            android:exported="false" />
    </application>

</manifest>